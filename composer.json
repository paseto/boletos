{
  "name": "paseto/boletos",
  "description": "Gerador de Boletos PDF",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "boleto",
    "pdf",
    "sicredi",
    "boleto-pdf",
    "bradesco",
    "itau",
    "caixa",
    "Banco do Brasil"
  ],
  "authors": [
    {
      "name": "Giovani Paseto",
      "email": "giovaniw2@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "itbz/fpdf": "~1.7",
    "mpdf/mpdf": "~5",
    "picqer/php-barcode-generator": "^0.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "squizlabs/php_codesniffer": "^2.8",
    "phpstan/phpstan": "^0.9.2"
  },
  "autoload": {
    "psr-4": {
      "Boleto\\": "src/Boleto"
    }
  },
  "scripts": {
    "phpcbf": "vendor/bin/phpcbf --standard=psr2 src/",
    "phpcs": "vendor/bin/phpcs --standard=psr2 src/",
    "phpstan": "vendor/bin/phpstan analyse src/ --level 1"
  }
}
